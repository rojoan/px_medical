<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=GBK">
        <title>餐饮服务许可证-正本打印</title>
        <script language="javascript" src="../app/print/LodopFuncs.js"></script>
            <link href="../app/print/designer.css" rel="stylesheet" type="text/css" />

<style>

body {
    font:20pt  宋体;
}

#page1 DIV {
    position:absolute;
}

/* 许可证编号 */
.f1{left:318mm;top:105mm;}
/* 企业名称 */
.f2{left:108mm;top:125mm;}
/* 经营范围  */
##.f3{left:108mm;top:185mm;}
/* 注册地址  */
.f4{left:108mm;top:156mm;}
/* 法定代表人 */
.f5{left:188mm;top:140mm;}
/* 发证机关  */
.f6{left:285mm;top:208mm;width:80mm;text-align: center;}
/* 起始有效期  */
.f7{left:112mm;top:240mm;}
/* 有效期至 */
.f8{left:184mm;top:240mm;}
/* 类别  */
.f9{left:108mm;top:172mm;}
/* 发证日期*/
.f10{left:287mm;top:226mm;}
/* 备注*/
.f11{left:108mm;top:188mm;}

</style>

<script language="JavaScript">

var LODOP; //声明为全局变量

function printPreview() {
    CreatePage();
    LODOP.PREVIEW();
}

function printDesign() {
    CreatePage();
    LODOP.PRINT_DESIGN();
};

function printSetup() {
    CreatePage();
    LODOP.PRINT_SETUP();
};

function CreatePage() {
    LODOP=getLodop();
    LODOP.PRINT_INIT("");
    LODOP.PRINT_INITA(0,0,1570,1123,"打印餐饮服务许可证");
    LODOP.SET_PRINT_PAGESIZE(2, 0, 0, "A3");
    LODOP.ADD_PRINT_SETUP_BKIMG("<img border='0' src='../app/licence/foodEntp.jpg'>");
    LODOP.SET_SHOW_MODE("BKIMG_LEFT",0);
    LODOP.SET_SHOW_MODE("BKIMG_TOP",0);
    LODOP.SET_SHOW_MODE("BKIMG_WIDTH",1570);
    LODOP.SET_SHOW_MODE("BKIMG_HEIGHT",1123);
    LODOP.SET_SHOW_MODE("BKIMG_IN_PREVIEW",true);
    LODOP.SET_PRINT_STYLE("FontSize",18);
    LODOP.ADD_PRINT_TEXT(401,999,214,34, "$!licenceNo");
    LODOP.SET_PRINT_STYLEA(0,"FontSize",14);
    LODOP.ADD_PRINT_TEXT(453,394,416,45,"$!entpName");
    LODOP.ADD_PRINT_TEXT(582,400,628,40,"$!registerAddress");
    LODOP.ADD_PRINT_TEXT(508,697,335,40,"$!corpName_01");
    LODOP.ADD_PRINT_TEXT(777,977,398,62,"$!depName");
    LODOP.SET_PRINT_STYLEA(0,"Alignment",2);
    LODOP.ADD_PRINT_TEXT(896,379,239,40,"$!fromDate.replace("-","    ")");
    LODOP.ADD_PRINT_TEXT(895,628,208,40,"$!toDate.replace("-","    ")");
    LODOP.ADD_PRINT_TEXT(690,395,483,188,"$!busName");
    LODOP.ADD_PRINT_TEXT(846,1060,220,40,"$!licenceTime.replace("-","    ")");
    LODOP.ADD_PRINT_TEXT(639,397,478,40,"$!busManner");

}

</script>

</head>

<body>

<div class="noprint" style="Z-INDEX: 100000; LEFT: 410px; WIDTH: 450px; POSITION: absolute; TOP: -10px;list-style:none;-moz-opacity:.75;filter:Alpha(opacity=75)">
  <fieldset class="search_border">
    <legend class="search_title"><font style="color:red;font-weight:bold">打印控制</font></legend>
      <center>
      <img style="CURSOR: hand" onclick="javascript:printPreview();" alt="打印预览" src="../app/print/images/icon_print.gif" border="0">
      <img style="CURSOR: hand" onclick="javascript:printDesign();" alt="打印设置" src="../app/print/images/icon_set.gif" border="0">
      <img style="CURSOR: hand" onclick="javascript:printSetup();" alt="保存打印格式" src="../app/print/images/icon_save_print.gif" border="0">

      <br>
    </center>
  </fieldset>
</div>

<div style="width:425mm;height:295mm;position:absolute;">
<img class="screen-only" src="../app/licence/foodEntp.jpg" style="width:425mm;height:295mm;position:absolute;">
</div>

<div id="page1" style="width:425mm;height:295mm;position:absolute;">
        <!-- 许可证编号 -->
        <div id="f1" draggable="true" class="f1" style="position:absolute;">$!licenceNo</div>
        <!-- 企业名称 -->
        <div id="f2" draggable="true" class="f2" style="position:absolute;">$!entpName</div>
        <!-- 经营范围 -->
        ##<div id="f3" draggable="true" class="f3" style="position:absolute;">$!busName</div>
        <!-- 注册地址 -->
        <div id="f4" draggable="true" class="f4" style="position:absolute;">$!registerAddress</div>
        <!-- 法定代表人 -->
        <div id="f5" draggable="true" class="f5" style="position:absolute;">$!corpName_01</div>
        <!-- 发证机关 -->
        <div id="f6" draggable="true" class="f6" style="position:absolute;">$!depName</div>
        <!-- 起始有效期 -->
        <div id="f7" draggable="true" class="f7" style="position:absolute;">$!fromDate.replace("-","&nbsp;&nbsp;&nbsp;")</div>
        <!-- 有效期至 -->
        <div id="f8" draggable="true" class="f8" style="position:absolute;">$!toDate.replace("-","&nbsp;&nbsp;&nbsp;")</div>
        <!-- 类别 -->
        <div id="f9" draggable="true" class="f9" style="position:absolute;">$!busManner</div>
        <!-- 发证日期 -->
        <div id="f10" draggable="true" class="f10" style="position:absolute;">$!licenceTime.replace("-","&nbsp;&nbsp;&nbsp;")</div>
        <!-- 备注 -->
        <div id="f11" draggable="true" class="f11" style="position:absolute;">$!busName</div>
</div>
</body>
</html>