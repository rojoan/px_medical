<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>新生儿家庭访视编辑</title> 
#parse("./common/pageCss.vm")
<style>
	html, body { height: 100% }
</style>
</head>
<body class="gray-bg-1">
	<form id="registerForm" method="get" class="form-horizontal" style="height: calc(100% - 44px)">
		#formHiddenInput("hospitalId" "$!childVisit.hospitalId" "id='hospitalId'")
		#formHiddenInput("hospitalNo" "$!childVisit.hospitalNo" "id='hospitalNo'")
		#formHiddenInput("childVisitId" "$!childVisit.childVisitId" "id='childVisitId'")
		#formHiddenInput("childDetailId" "$!childVisit.childDetailId" "id='childDetailId'")
		<div class="modal-body full-height-scroll">
			<!--如果弹窗没有底部菜单栏则 -44px -->
			<!-- 表单 开始 -->
			<div class="table-content">
				<div class="text-box">
					<h5>基本情况</h5>
				</div>
				<div class="form-group">
					<label class="col-sm-1 control-label text-right p-r-none p-l-none must-red">新生儿姓名</label>
					<div class="col-sm-3">
						#formTextInput("childName" "#htmlEscape($!childVisit.childName)" " id='childName' maxlength='10' class='form-control'")
					</div>
					<label class="col-sm-1 control-label text-right p-r-none p-l-none">性别</label>
					<div class="col-sm-3" style="padding-top:6px;">
						#formRadioButtons("gender" "$!childVisit.gender" $!dicData.dicSex "" "class='checkbox-inline i-checks'")
					</div>
					<label class="col-sm-1 control-label text-right p-r-none p-l-none">妊娠周数(周)</label>
					<div class="col-sm-2">
						#formIntZeroInput("fetationAge" "$!childVisit.fetationAge" "id='fetationAge' maxlength='2' class='form-control' readOnly")
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-1 control-label text-right p-r-none p-l-none">出生体重(kg)</label>
					<div class="col-sm-3">
						#formFloLenInput("childBirthWeight" "$!childVisit.childBirthWeight" 3 "id='birthWeight' maxlength='5' class='form-control' readOnly")
					</div>
					<label class="col-sm-1 control-label text-right p-r-none p-l-none">出生身长(cm)</label>
					<div class="col-sm-3">
						#formFloLenInput("childBirthHeight" "$!childVisit.childBirthHeight" 3 "id='birthHeight' maxlength='5' class='form-control' readOnly")
					</div>
					<label class="col-sm-1 control-label text-right p-r-none p-l-none">出生日期</label>
					<div class="col-sm-3">
						#set($dateFormat = '"YYYY-MM-DD hh:mm:ss"')
						#set($nowTimeFormat = '"YYYY-MM-DD hh:mm:ss"')
						#formTextInput("birthDate" "$!date.format('yyyy-MM-dd HH:mm:ss', $!childVisit.birthDate)" " id='birthDate' class='input-sm form-control layer-date' onclick='laydate({istime: true, start: laydate.now(0, $!nowTimeFormat), format: $!dateFormat})' readOnly")
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-1 control-label text-right p-r-none p-l-none">随访医生</label>
					<div class="col-sm-3">
						#if("$!childVisit.visitDoctor" != "")
							#formTextInput("visitDoctor" "$!childVisit.visitDoctor" " id='visitDoctor' class='form-control' readOnly")
						#else
							#formTextInput("visitDoctor" "$!{currentUser.name}" " id='visitDoctor' class='form-control' readOnly")
						#end
					</div>
					<label class="col-sm-1 control-label text-right p-r-none p-l-none">医生联系电话</label>
					<div class="col-sm-3">
						#formTextInput("visitDoctorPhone" "#htmlEscape($!childVisit.visitDoctorPhone)" " id='visitDoctorPhone' class='form-control'")
					</div>
					<label class="col-sm-1 control-label text-right p-r-none p-l-none must-red">随访日期</label>
					<div class="col-sm-3">
						#set($dateFormat = '"YYYY-MM-DD hh:mm:ss"')
						#formTextInput("visitDate" "$!date.format('yyyy-MM-dd HH:mm:ss', $!childVisit.birthDate)" " id='visitDate' class='input-sm form-control layer-date' onclick='laydate({istime: true, start: laydate.now(0, $!nowTimeFormat), format: $!dateFormat})' readOnly")
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-1 control-label text-right p-r-none p-l-none">助产机构</label>
					<div class="col-sm-3">
						#formTextInput("hospitalName" "#htmlEscape($!childVisit.hospitalName)" " id='hospitalName' class='form-control' readOnly")
					</div>
					<label class="col-sm-1 control-label text-right p-r-none p-l-none must-red">下次随访地址</label>
					<div class="col-sm-3">
						#formTextInput("nextVisitAddr" "#htmlEscape($!childVisit.nextVisitAddr)" "id='nextVisitAddr' maxlength='50' class='form-control'")
					</div>
					<label class="col-sm-1 control-label text-right p-r-none p-l-none">下次随访日期</label>
					<div class="col-sm-3">
						#set($dateFormat = '"YYYY-MM-DD hh:mm:ss"')
						#formTextInput("nextVisitTime" "$!date.format('yyyy-MM-dd HH:mm:ss', $!childVisit.nextVisitTime)" " id='nextVisitTime' class='input-sm form-control layer-date' onclick='laydate({istime: true, start: laydate.now(0, $!nowTimeFormat), format: $!dateFormat})' readOnly")
					</div>
				</div>
				<div class="text-box">
					<h5>检查结果</h5>
				</div>
				<div class="form-group">
					<label class="col-md-1 control-label text-right p-r-none">目前体重(kg)</label>
					<div class="col-md-3">
						#formFloLenInput("childCurWeight" "$!childVisit.childCurWeight" 3 "maxlength='5' class='form-control'")
					</div>
					<label class="col-md-1 control-label text-right p-r-n">目前身高（cm）</label>
					<div class="col-md-3">
						#formFloLenInput("childCurHeight" "$!childVisit.childCurHeight" 3 "maxlength='5' class='form-control'")
					</div>
					<label class="col-md-1 control-label text-right p-r-none">呕吐</label>
					<div class="col-md-3" style="padding-top:6px;">
						#formRadioButtons("emesisExam" "$!childVisit.emesisExam" $!dicData.dicHaveNo "" "class='checkbox-inline i-checks'")
					</div>
				</div>
				<div class="form-group">
					<label class="col-md-1 control-label text-right p-r-none">喂养方式$!childVisit.feedFlag</label>
					<div class="col-md-3">
						#formSingleSelectFirstNone("feedFlag" "$!childVisit.feedFlag" $!{dicData.dicFeedFlag} "class='form-control'" "")
					</div>
					<label class="col-md-1 control-label text-right p-r-none">吃奶量(ml/次)</label>
					<div class="col-md-3">
						#formIntZeroInput("suckMl" "$!childVisit.suckMl" "maxlength='3' class='form-control'")
					</div>
					<label class="col-md-1 control-label text-right p-r-n">吃奶次数(次/日)</label>
					<div class="col-md-3">
						#formIntZeroInput("suckPer" "$!childVisit.suckPer" "maxlength='2' class='form-control'")
					</div>
				</div>
				<div class="form-group">
					<label class="col-md-1 control-label text-right p-r-none">体温(℃)</label>
					<div class="col-md-3">
						#formFloLenInput("animalHeat" "$!childVisit.animalHeat" 3 "maxlength='5' class='form-control'")
					</div>
					<label class="col-md-1 control-label text-right p-r-none">心率(ml/分)</label>
					<div class="col-md-3">
						#formIntZeroInput("heartRate" "$!childVisit.heartRate" "maxlength='5' class='form-control'")
					</div>
					<label class="col-md-1 control-label text-right p-r-n ">呼吸频率(次/分)</label>
					<div class="col-md-3">
						#formIntZeroInput("breatheRate" "$!childVisit.breatheRate" "maxlength='5' class='form-control'")
					</div>
				</div>
				<div class="form-group">
					<label class="col-md-1 control-label text-right p-r-none">大便</label>
					<div class="col-md-3">
						#formSingleSelectFirstNone("shitExam" "$!childVisit.shitExam" $!{dicData.dicShitExam} "class='form-control'" "")
					</div>
					<label class="col-md-1 control-label text-right p-r-n ">大便次数(次/日)</label>
					<div class="col-md-3">
						#formIntZeroInput("shitDayPer" "$!childVisit.shitDayPer" "maxlength='2' class='form-control'")
					</div>
					<label class="col-md-1 control-label text-right p-r-none">大便其他描述</label>
					<div class="col-md-3">
						#formTextInput("shitDescribe" "#htmlEscape($!childVisit.shitDescribe)" " maxlength='50' class='form-control'")
					</div>
				</div>
				<div class="form-group">
					<label class="col-md-1 control-label text-right p-r-none">面色</label>
					<div class="col-md-2">
						#formSingleSelectFirstNone("complexion" "$!childVisit.complexion" $!{dicData.dicComplexion} "class='form-control'" "")
					</div>
					<div class="col-md-3">
						#formTextInput("complexionOrther" "#htmlEscape($!childVisit.complexionOrther)" " maxlength='50' class='form-control'")
					</div>
					<label class="col-md-1 control-label text-right p-r-none">黄疸部位</label>
					<div class="col-md-5" style="padding-top: 6px;">
						#formCheckboxesLable("jaundiceParts" $!jaundiceParts $!dicData.dicJaundiceParts "&nbsp;" "class='m-t-xs m-r-xs i-checks'" "")
					</div>
				</div>
				<div class="form-group">
					<label class="col-md-1 control-label text-right p-r-none">前囟</label>
					<div class="col-md-2">
						#formSingleSelectFirstNone("fontanelle" "$!childVisit.fontanelle" $!{dicData.dicFontanelle} "class='form-control'" "")
					</div>
					<div class=" input-group col-sm-2" style="padding-left: 9px; display: inline-table; float: left">
						#formFloLenInput("fontanelleAcross" "$!childVisit.fontanelleAcross" 3 "maxlength='5' class='form-control'")
						<span class="input-group-addon">/</span> 
						#formFloLenInput("fontanelleVertical" "$!childVisit.fontanelleVertical" 3 "maxlength='5' class='form-control'")
					</div>
					<label class="col-md-1 control-label text-left p-r-none">cm</label>
					<label class="col-md-1 control-label text-right p-r-none">前囟其他</label>
					<div class="col-md-5">
						#formTextInput("fontanelleOrther" "#htmlEscape($!childVisit.fontanelleOrther)" " maxlength='50' class='form-control'")
					</div>
				</div>

				<div class="form-group">
					<label class="col-md-1 control-label text-right p-r-none">眼睛</label>
					<div class="col-md-2">
						#formSingleSelectFirstNone("eyeExam" "$!childVisit.eyeExam" $!{dicData.dicExceptional} "class='form-control'" "")
					</div>
					<div class="col-md-3">
						#formTextInput("eyeDescribe" "#htmlEscape($!childVisit.eyeDescribe)" " maxlength='50' class='form-control'")
					</div>
					<label class="col-md-1 control-label text-right p-r-none">四肢活动度</label>
					<div class="col-md-2">
						#formSingleSelectFirstNone("armLeg" "$!childVisit.armLeg" $!{dicData.dicExceptional} "class='form-control'" "")
					</div>
					<div class="col-md-3">
						#formTextInput("armLegDescribe" "#htmlEscape($!childVisit.armLegDescribe)" " maxlength='50' class='form-control'")
					</div>
				</div>
				<div class="form-group">
					<label class="col-md-1 control-label text-right p-r-none">耳外观</label>
					<div class="col-md-2">
						#formSingleSelectFirstNone("earExam" "$!childVisit.earExam" $!{dicData.dicExceptional} "class='form-control'" "")
					</div>
					<div class="col-md-3">
						#formTextInput("earDescribe" "#htmlEscape($!childVisit.earDescribe)" " maxlength='50' class='form-control'")
					</div>
					<label class="col-md-1 control-label text-right p-r-none">颈部包块</label>
					<div class="col-md-2">
						#formSingleSelectFirstNone("neckExam" "$!childVisit.neckExam" $!{dicData.dicExceptional} "class='form-control'" "")
					</div>
					<div class="col-md-3">
						#formTextInput("neckDescribe" "#htmlEscape($!childVisit.neckDescribe)" " maxlength='50' class='form-control'")
					</div>
				</div>

				<div class="form-group">
					<label class="col-md-1 control-label text-right p-r-none">鼻</label>
					<div class="col-md-2">
						#formSingleSelectFirstNone("noseExam" "$!childVisit.noseExam" $!{dicData.dicExceptional} "class='form-control'" "")
					</div>
					<div class="col-md-3">
						#formTextInput("noseDescribe" "#htmlEscape($!childVisit.noseDescribe)" " maxlength='50' class='form-control'")
					</div>
					<label class="col-md-1 control-label text-right p-r-none">皮肤</label>
					<div class="col-md-2">
						#formSingleSelectFirstNone("skinExam" "$!childVisit.skinExam" $!{dicData.dicExceptional} "class='form-control'" "")
					</div>
					<div class="col-md-3">
						#formTextInput("skinDescribe" "#htmlEscape($!childVisit.skinDescribe)" " maxlength='50' class='form-control'")
					</div>
				</div>
				<div class="form-group">
					<label class="col-md-1 control-label text-right p-r-none">口腔</label>
					<div class="col-md-2">
						#formSingleSelectFirstNone("mouthExam" "$!childVisit.mouthExam" $!{dicData.dicExceptional} "class='form-control'" "")
					</div>
					<div class="col-md-3">
						#formTextInput("mouthDescribe" "#htmlEscape($!childVisit.mouthDescribe)" " maxlength='50' class='form-control'")
					</div>
					<label class="col-md-1 control-label text-right p-r-none">肛门</label>
					<div class="col-md-2">
						#formSingleSelectFirstNone("anusExam" "$!childVisit.anusExam" $!{dicData.dicExceptional} "class='form-control'" "")
					</div>
					<div class="col-md-3">
						#formTextInput("anusDescribe" "#htmlEscape($!childVisit.anusDescribe)" " maxlength='50' class='form-control'")
					</div>
				</div>
				<div class="form-group">
					<label class="col-md-1 control-label text-right p-r-none">心肺听诊</label>
					<div class="col-md-2">
						#formSingleSelectFirstNone("heartLungExam" "$!childVisit.heartLungExam" $!{dicData.dicExceptional} "class='form-control'" "")
					</div>
					<div class="col-md-3">
						#formTextInput("heartLungDescribe" "#htmlEscape($!childVisit.heartLungDescribe)" " maxlength='50' class='form-control'")
					</div>
					<label class="col-md-1 control-label text-right p-r-none">胸部</label>
					<div class="col-md-2">
						#formSingleSelectFirstNone("chestExam" "$!childVisit.chestExam" $!{dicData.dicExceptional} "class='form-control'" "")
					</div>
					<div class="col-md-3">
						#formTextInput("chestDescribe" "#htmlEscape($!childVisit.chestDescribe)" " maxlength='50' class='form-control'")
					</div>
				</div>
				<div class="form-group">
					<label class="col-md-1 control-label text-right p-r-none">腹部触诊</label>
					<div class="col-md-2">
						#formSingleSelectFirstNone("abdominalExam" "$!childVisit.abdominalExam" $!{dicData.dicExceptional} "class='form-control'" "")
					</div>
					<div class="col-md-3">
						#formTextInput("abdominalDescribe" "#htmlEscape($!childVisit.abdominalDescribe)" " maxlength='50' class='form-control'")
					</div>
					<label class="col-md-1 control-label text-right p-r-none">脊柱</label>
					<div class="col-md-2">
						#formSingleSelectFirstNone("spineExam" "$!childVisit.spineExam" $!{dicData.dicExceptional} "class='form-control'" "")
					</div>
					<div class="col-md-3">
						#formTextInput("spineDescribe" "#htmlEscape($!childVisit.spineDescribe)" " maxlength='50' class='form-control'")
					</div>
				</div>
				<div class="form-group">
					<label class="col-md-1 control-label text-right p-r-none">脐部</label>
					<div class="col-md-2">
						#formSingleSelectFirstNone("navelExam" "$!childVisit.navelExam" $!{dicData.dicExceptional} "class='form-control'" "")
					</div>
					<div class="col-md-3">
						#formTextInput("navelDescribe" "#htmlEscape($!childVisit.navelDescribe)" " maxlength='50' class='form-control'")
					</div>
				</div>
				<div class="form-group">
					<label class="col-md-1 control-label text-right p-r-none">随访指导</label>
					<div class="col-md-11">
						#formCheckboxesLable("visitAdivseIds" $!adviseIdList $!dicData.dicNewVisitAdvise "&nbsp;" "class='m-t-xs m-r-xs i-checks'" "")
					</div>
				</div>
			</div>
			<!-- 表单 结束 -->
		</div>
		<div class="modal-footer tool-table-fixed" style="text-align: center; width: calc(100% - 15px)">
			<button type="button" id="btn_register_save" class="btn btn-success">
				<i class="fa fa-paste"></i> 保 存
			</button>
		</div>
	</form>
</body>
<!-- 全局js -->
#parse("./common/pageJs.vm") 
#parse("./common/common.js.vm")
#parse("./publichealth/newChildVisitEdit.js.vm")
</html>